<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="br.com.offcina.control.MainFXMLController">
   <center>
      <AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" styleClass="mainFxmlClass">
          <stylesheets>
              <URL value="@/styles/Styles.css" />
          </stylesheets>
         <children>
            <Button layoutX="36.0" layoutY="43.0" mnemonicParsing="false" onAction="#handleCargoListar" prefHeight="25.0" prefWidth="67.0" text="Listar" />
            <Button layoutX="36.0" layoutY="80.0" mnemonicParsing="false" onAction="#handleCargoCadastrar" text="Cadastrar" />
         </children>
      </AnchorPane>
   </center>
   <top>
      <MenuBar BorderPane.alignment="CENTER">
        <menus>
          <Menu mnemonicParsing="false" text="Equipe">
            <items>
              <MenuItem mnemonicParsing="false" onAction="#handleEquipeListar" text="Listar" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
   </top>
</BorderPane>
